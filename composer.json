{
  "name": "edgetelemetrics/eventcorrelation",
  "description": "Event Stream correlation engine for PHP",
  "keywords": ["php-ec", "event", "stream"],
  "license": "MIT",
  "authors": [ { "name": "James Lucas", "email": "james@lucas.net.au" } ],
  "type": "library",
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "ext-pcntl": "*",
    "evenement/evenement": "^3.0.1",
    "edgetelemetrics/php-json-rpc": "^1.0.2",
    "edgetelemetrics/timebucket": "^0.1.0",
    "clue/ndjson-react": "^1.1.0",
    "react/child-process": "^0.5.2",
    "react/event-loop": "^1.1.1",
    "react/filesystem": "^0.1.2"
  },
  "suggest": {
    "ext-zend-opcache": "*"
  },
  "require-dev": {
    "react/http": "^0.8.7",
    "nikic/php-parser" : "*",
    "phpstan/phpstan" : "*",
    "kelunik/fqn-check": "^0.1.3"
  },
  "autoload": {
    "psr-4": {
      "EdgeTelemetrics\\EventCorrelation\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  }
}
