{
  "name": "edgetelemetrics/eventcorrelation",
  "description": "Event Stream correlation engine for PHP",
  "keywords": ["php-ec", "event", "stream"],
  "license": "MIT",
  "authors": [ { "name": "James Lucas", "email": "james@lucas.net.au" } ],
  "type": "library",
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "evenement/evenement": "^3.0",
    "symfony/polyfill-php73": "^1.10",
    "edgetelemetrics/php-json-rpc": "^1.0.1",
    "edgetelemetrics/timebucket": "dev-master"
  },
  "require-dev": {
    "ext-pcntl": "*",
    "clue/ndjson-react": "^1.0",
    "react/child-process": "^0.5.2",
    "react/event-loop": "^1.0",
    "react/http": "^0.8.3",
    "react/filesystem": "^0.1.2",
    "nikic/php-parser" : "*",
    "phpstan/phpstan" : "*",
    "kelunik/fqn-check": "^0.1.3"
  },
  "suggest": {
    "ext-eio": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "EdgeTelemetrics\\EventCorrelation\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "autoload-dev": {
    "classmap": [ "tests/EventProcessors" ]
  }
}
